(setq l 2)

(func factorial (n)
  (cond
      (less n l)
        1
        (* n (factorial ((- n 1 )) ) )
        )
  )

(setq n 5)

(factorial (n))

((lambda (x)
(cond
      (less x 2)
        1
        (* x (factorial ((- x 1 )) ) )
        )
) (3))

(setq facn '(factorial (4)))

(eval facn)

(setq myFunc (lambda (y) (* 2 y)))

(myFunc (4))